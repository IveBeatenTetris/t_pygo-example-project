<?xml version="1.0"?>
<interface test="test" background="25, 25, 35">
  <menubar name="menu_main" rect="0, 0, 100%, 25" background="45, 45, 55">
    <menu name="File">
      <option name="New" call="file_create" />
      <option name="Open" call="file_open" />
      <option name="Close" call="file_close" />
    </menu>
    <menu name="Edit">
      <option name="Test Function" call="testFunction" />
    </menu>
    <menu name="View" />
    <menu name="Help" />
    <menu name="[Unnamed]" />
  </menubar>
  <panel name="panel_main" rect="0, 25, 20%, 100%" background="35, 35, 45">
    <button name="windowopen1" background="25, 25, 35" hover="35, 35, 45"
      rect="10, 10, 120, 25" call="open_window" fontsize="12"
      text="Open a new Window" />
  </panel>
  <infobar name="bar_info" text="Open a new Window" rect="0, bottom, 100%, 25"
    background="45, 45, 55" info="mouse_loc, app_size, app_fps" />
  <!--<button name="button_test1" text="Drag me" drag="true" hover="55, 55, 65"
    rect="300, 100, 100, 36" background="35, 35, 45" bold="true" />
  <button name="call_popup" text="Call a popup" background="35, 35, 45"
    hover="55, 55, 65" rect="500, 200, 100, 36" call="call_popup" />-->
</interface>
